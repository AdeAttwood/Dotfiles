import { LitElement, html, css } from 'lit-element';
import { customElement, property } from 'lit-element/decorators.js';

@customElement('${1:x-component}')
export class Component extends LitElement {
  static get styles() {
    return css\`:host {}\`;
  }

  render() {
    return html\`<div>Content</div>\`;
  }
}
